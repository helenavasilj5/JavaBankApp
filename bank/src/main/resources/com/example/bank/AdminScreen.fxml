        </columns>
      </TableView>
      <Button fx:id="btn" layoutX="8.0" layoutY="263.0" mnemonicParsing="false" onAction="#checkUser" prefHeight="29.0" prefWidth="132.0" text="Check Selected User" />
      <Button fx:id="btn2" layoutX="8.0" layoutY="357.0" mnemonicParsing="false" onAction="#logout" prefHeight="29.0" prefWidth="132.0" text="Logout" />
      <Button fx:id="btnLoginHistory" layoutX="8.0" layoutY="310.0" mnemonicParsing="false" onAction="#loginHistory" prefHeight="29.0" prefWidth="132.0" text="Login History" />
