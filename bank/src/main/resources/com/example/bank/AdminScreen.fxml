          <TableColumn fx:id="col1" prefWidth="115.0" text="Name" />
          <TableColumn fx:id="col2" prefWidth="136.0" text="Username" />
            <TableColumn fx:id="col3" prefWidth="85.0" text="Role" />
            <TableColumn fx:id="col4" prefWidth="177.0" text="Email" />
