<AnchorPane prefHeight="400.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bank.controller.AdminScreenController">
   <children>
      <TableView fx:id="tbl" layoutX="143.0" layoutY="15.0" prefHeight="371.0" prefWidth="551.0">
